import pyautogui
import time

pyautogui.hotkey('win','r')
time.sleep(0.01)
pyautogui.write('chrome')
time.sleep(0.1)
pyautogui.press('enter')
for i in range(10):
    time.sleep(0.5)
    pyautogui.write('https://media.tenor.com/gQV5VzHLWQIAAAAd/among-us-sus.gif')
    time.sleep(0.1)
    pyautogui.press('enter')
    time.sleep(0.01)
    pyautogui.hotkey('ctrl','n')
   
