import pyautogui
import time
n = int(input())
for i in range(n):
    pyautogui.hotkey('win','r')
    time.sleep(0.1)
    pyautogui.write('notepad')
    time.sleep(0.1)
    pyautogui.press('enter')
    time.sleep(0.1)
    pyautogui.hotkey('ctrl','s')
    time.sleep(0.1)
    pyautogui.write(str(i+1))
    time.sleep(0.1)
    pyautogui.press('enter')
    time.sleep(0.1)
    pyautogui.hotkey('alt','f4')
    time.sleep(0.1)
